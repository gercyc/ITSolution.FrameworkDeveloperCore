<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Authors>Gercy Campos</Authors>
    <FileVersion>1.0.0.5</FileVersion>
    <Version>1.0.5</Version>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageVersion>1.0.6</PackageVersion>
    <Title>Classes comuns</Title>
    <Description>Classes comuns para utilização durante desenvolvimento. </Description>
    <RepositoryUrl>https://github.com/gercyc/ITSolution.FrameworkDeveloperCore</RepositoryUrl>
    <SignAssembly>false</SignAssembly>
    <Nullable>disable</Nullable>
    <AssemblyName>ITSolution.Framework.Core.Common</AssemblyName>
    <RootNamespace>ITSolution.Framework.Core.Common</RootNamespace>
